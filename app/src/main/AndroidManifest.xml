<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.andre_home.treinamento2">

    <application
        android:debuggable="true"
        android:allowBackup="true"
        android:icon="@drawable/ic_flash_on_black_24dp"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_flash_on_black_24dp"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activities.SingUp"
            android:screenOrientation="portrait">
            <intent-filter>
                <action
                    android:name="android.intent.action.MAIN" />

                <category
                    android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".activities.Tela_Cadastro"
            android:screenOrientation="portrait">

        </activity>
    </application>

</manifest>